<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <AssemblyName>maskx.OrchestrationService</AssemblyName>
    <RootNamespace>maskx.OrchestrationService</RootNamespace>
    <Authors>maskx</Authors>
    <Version>3.1.9.9</Version>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/maskx/OrchestrationService</PackageProjectUrl>
    <RepositoryUrl>https://github.com/maskx/OrchestrationService</RepositoryUrl>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="maskx.DurableTask.SQLServer" Version="0.1.2" />
    <PackageReference Include="Microsoft.Azure.DurableTask.Core" Version="2.4.1" />
    <PackageReference Include="Microsoft.Extensions.Hosting" Version="3.1.8" />
    <PackageReference Include="Microsoft.Extensions.Http.Polly" Version="3.1.8" />
    <PackageReference Include="Polly.Contrib.WaitAndRetry" Version="1.1.1" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.2" />
  </ItemGroup>
</Project>